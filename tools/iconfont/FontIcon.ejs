import { createFromIconfontCN } from '@ant-design/icons/lib'
import { IconBaseProps } from '@ant-design/icons/lib/components/Icon'

// eslint-disable-next-line
export type FontIconType = <%- fontIconTypes %>

interface FontIconProps extends Omit<IconBaseProps, 'type'> {
  type: FontIconType
}

const FontIcon = createFromIconfontCN({
